<% provide(:title, 'Użytkownicy') %>
<h1>Wszyscy użytkownicy</h1>


 <div class="color">
<%= form_tag users_path, :method => 'get' do %>
  <p>
    <%= text_field_tag :search, params[:search] %>
    <%= submit_tag " Znajdź osobę ",class: "btn btn-primary", :name => nil %>
  </p>
<% end %>
</div>
<ul class="users">
  <%= render @users %>
</ul>


